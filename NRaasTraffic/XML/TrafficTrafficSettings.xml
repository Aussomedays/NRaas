      <!--Whether to require parking spaces for food trucks-->
    </kRequireFoodParkingSpace>
    <kMaxFoodTrucks value="2">
      <!--The number of food trucks to spawn in town-->
    </kMaxFoodTrucks>
    <kAllowFoodTruckActiveLot value="False">
      <!--Whether to allow the food truck to visit the active lot-->
    </kAllowFoodTruckActiveLot>
    <kAllowFoodTruckResidential value="False">
      <!--Whether to allow the food truck to visit residential lots-->
    </kAllowFoodTruckResidential>
