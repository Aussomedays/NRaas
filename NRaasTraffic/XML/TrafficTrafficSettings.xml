      <!--The delta change from the EA defined traffic values-->
    </kTrafficDelta>
    <kEnableIceCreamTruckVacation value="False">
      <!--Whether to run the ice cream truck manager while on vacation-->
    </kEnableIceCreamTruckVacation>
    <kMaxIceCreamTrucks value="2">
      <!--The number of ice cream trucks to spawn in town-->
    </kMaxIceCreamTrucks>
    <kAllowIceCreamActiveLot value="True">
      <!--Whether to allow the ice cream truck to visit the active lot-->
    </kAllowIceCreamActiveLot>
    <kAllowIceCreamResidential value="True">
      <!--Whether to allow the ice cream truck to visit residential lots-->
    </kAllowIceCreamResidential>
    <kAllowIceCreamAtNight value="False">
      <!--Whether to allow the ice cream truck at night-->
    </kAllowIceCreamAtNight>
